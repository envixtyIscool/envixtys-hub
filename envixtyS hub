local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("envixtys hub", "BloodTheme")
    -- MAIN
    local Main = Window:NewTab("universal")
    local MainSection = Main:NewSection("universal")    -- MAIN
    
    
    MainSection:NewButton("universal reach script", "hit your enemies from farther away", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/x10-steal-time-from-others-and-flex-your-sword-Reach-Script-5437"))()
    end)
    
    MainSection:NewButton("Back/Front Flip", "Makes you do gymnastics", function()
        loadstring(game:HttpGet('https://pastebin.com/raw/7wDcPtLk'))()
    end)

    MainSection:NewToggle("Super-Human", "go fast and jump high", function(state)
        if state then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
        else
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        end
    end)

    MainSection:NewButton("Infinite Yield", "FE Admin Commands", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
    end)
    
 
 
 
 --blox fruits
 
    local bloxfruits = Window:NewTab("bloxfruits")
    local bloxfruitsSection = bloxfruits:NewSection("bloxfruits")

    bloxfruitsSection:NewButton("blox fruits hub", "a blox fruits script that alows you to overnight farm to rain fruits fron to heavens",  function()
       loadstring(game:HttpGet('https://raw.githubusercontent.com/SaraSenpai/bloxfruist/main/Sarahub'))()
    end)
    
    bloxfruitsSection:NewButton("blox fruits", "autofam script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Minhtriettt/Free-Script/main/MTriet-Hub.lua"))()
    end)



    
    local bladeball = Window:NewTab("bladeball")
    local bladeballSection = bladeball:NewSection("bladeball")

   bladeballSection:NewButton("bedbol hub", "a blade ball hub with multiple features like auto parry freeze ball lag server and more", function()
    _G.UI_Size = 200 
loadstring(game:HttpGet("https://raw.githubusercontent.com/3345-c-a-t-s-u-s/-beta-/main/AutoParry.lua"))()
end)
  
bladeballSection:NewButton("inferno hub", "a popular bladeball hub not keyless", function()
loadstring(game:HttpGet("https://github.com/SadlekAski/Scripts/raw/main/Blade%20Ball/Equip%20any%20ability.lua"))()
end)

bladeballSection:NewButton("autoblock hub", "auto block hub with a visualiser", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/autoparrybest"))()
end) 
